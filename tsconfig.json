{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "declaration": false,
    "noImplicitAny": false,
    "removeComments": true,
    "noLib": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "strictNullChecks": false,
    "noUnusedParameters": false,
    "noUnusedLocals": true
  },
  "exclude": [
    "node_modules",
    "static"
  ]
}
